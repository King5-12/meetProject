{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "importHelpers": true,
        "jsx": "react",
        "esModuleInterop": true,
        "sourceMap": true,
        "baseUrl": "./",
        "strict": true,
        "strictNullChecks": false,
        "paths": {
            "@/*": ["src/*"],
            "@@/*": ["src/.umi/*"]
        },
        "allowSyntheticDefaultImports": true,
        "noImplicitThis": false
    },
    "include": [
        "mock/**/*",
        "src/**/*",
        "config/**/*",
        ".umirc.ts",
        "typings.d.ts"
    ]
}
